{
    "name":                 "edify.markdown",
    "version":              "0.0.4",
    "description":

      "Markdown generator for static JavaScript.",

    "keywords":
    [
                            "documentation", "api", "docs", "cms", "website",
                            "github", "pages", "html", "markdown", "docco"
    ],
    "author":               "Alan Gutierrez <alan@prettyrobots.com>",
    "homepage":             "https://github.com/bigeasy/edify",
    "bugs":                 "https://github.com/bigeasy/edify/issues",
    "license":              "MIT",
    "repository":
    {
                            "type": "git",
                            "url": "bigeasy/edify"
    },
    "dependencies":
    {
                            "arguable":                     "7.0.x",
                            "cadence":                      "3.0.x",
                            "cheerio":                      "1.0.0-rc.2",
                            "delta":                        "3.0.x",
                            "marked":                       "0.6.0"
    },
    "devDependencies":
    {
                            "proof":                        "5.0.x"
    },
    "main":                 "markdown.bin",
    "scripts":
    {
                            "test": "(proof run t/*.t.js | tee .proof.out | proof progress) || (proof errors < .proof.out)"
    }
}
