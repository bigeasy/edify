{ "name" : "edify"
, "description" : "Documentation generator for GitHub:Pages."
, "url" : "http://bigeasy.github.com/edify"
, "keywords" : ["documentation", "api", "docs", "cms", "website", "github", "pages", "html", "markdown", "docco"]
, "author" : "Alan Gutierrez <alan@prettyrobots.com>"
, "contributors" : []
, "devDependencies":
  { "proof": ">=0.0.1"
  }
, "dependencies":
  { "streamline": ">=0.2.4"
  , "coffee-script": ">=1.1.3"
  }
, "lib" : "lib"
, "main" : "./lib/edify"
, "scripts" : { "test": "(proof run t/*/*.t | tee .proof.out | proof progress) || (proof errors < .proof.out)" }
