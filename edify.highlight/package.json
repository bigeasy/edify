    "homepage":         "http://bigeasy.github.com/edify",
    "keywords":
    [
                        "documentation", "api", "docs", "cms", "website",
                        "github", "pages", "html", "highlight", "docco"
    ],
    "author":           "Alan Gutierrez <alan@prettyrobots.com>",
    "contributors":     [],
    "homepage":         "https://github.com/bigeasy/edify",
